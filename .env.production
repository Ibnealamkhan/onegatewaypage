# Production Environment Variables for aaPanel Deployment
NODE_ENV=production
PORT=3000

# Supabase Configuration (Replace with your actual values)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Analytics Configuration (Replace with your actual IDs)
VITE_GA_MEASUREMENT_ID=your_google_analytics_id_here
VITE_FB_PIXEL_ID=your_facebook_pixel_id_here

# Domain Configuration
VITE_DOMAIN=onegateway.in
VITE_API_URL=https://onegateway.in/api

# Security Headers
VITE_ENABLE_SECURITY_HEADERS=true

# Performance Optimization
VITE_ENABLE_GZIP=true
VITE_ENABLE_CACHE=true